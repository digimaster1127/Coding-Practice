#include <iostream>

using namespace std;

int main()
{
  int x,y,z;
  x = 50;
  y = 100;
  z = x + y;

cout << " z = " << z << endl;

  int sum = 100;
  int sum2 = 100 + sum;
  int sum3 = sum2 * 2;
  float sum4 = 3.14159;
  double sum5 = (sum+sum2)*(sum3/sum4);
  cout << " sum3 = " << sum3 << " and sum5 = " << sum5 <<endl;
  
return 0;
}
